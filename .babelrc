{
  "sourceMaps": "both",
  "presets": [
    ["@emotion/babel-preset-css-prop", {"sourceMap": true}],
    "@babel/react",
    "@babel/preset-typescript",
    ["@babel/env", {"useBuiltIns": "usage"}],
    "minify"
  ],
  "plugins": [
    ["module-resolver", {"alias": {"^@internal/(.+)$": "./@internal/\\1"}}],
    "@babel/plugin-syntax-dynamic-import",
    "annotate-console-log"
  ]
}
